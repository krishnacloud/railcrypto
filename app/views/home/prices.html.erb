<h1>this is price page</h1>

<% if  @symbol %>
	<% if @quote["Response"] %>
		<h4> The symbol <%= @symbol %> Dosent exist.</h4>
	<% else %>
			<% @quote["DISPLAY"].each do |key, value| %>
			<div class="jumbotron">
			  <h1 class="display-4"><%= key %></h1>
			  	Price <%= value["USD"]["PRICE"] %><br/>
				24hr High: <%= value["USD"]["HIGHDAY"] %><br/>
				24hr Low: <%= value["USD"]["LOWDAY"] %><br/>
				Market Cap: <%= value["USD"]["MKTCAP"] %><br/>
			</div>
	<% end %>
 <% end %>
<% end %>